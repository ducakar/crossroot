/dev/sda	/		ext3		noatime					0	1
proc		/proc		proc		noatime,nosuid,nodev,noexec		0	0
sysfs		/sys		sysfs		noatime,nosuid,nodev,noexec		0	0
#dev		/dev		devtmpfs	noatime,nosuid,mode=0755		0	0
devpts		/dev/pts	devpts		noatime,nosuid,noexec,gid=5,mode=620	0	0
tmpfs		/dev/shm	tmpfs		noatime,nosuid,nodev			0	0
tmpfs		/tmp		tmpfs		noatime				0	0
tmpfs		/run		tmpfs		noatime,nosuid,nodev,mode=0755		0	0
